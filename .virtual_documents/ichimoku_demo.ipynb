    plt.fill_between(df['timestamp'], df['senkou_span_1'], df['senkou_span_2'], 
                     where=(df['senkou_span_1'] < df['senkou_span_2']),
                     color='lightcoral', alpha=0.3, label='Sell')

plt.legend()

# Add axis labels and a title
plt.xlabel('Date')
plt.ylabel('Price')
